package ejercicio;

import java.lang.Math;

//Calcular el cuadrado de los números del 1 al  6, mostrar el número con su cuadrado
public class ejercicio2 {
	public static void main (String [] args) {
		System.out.println("el cuadrado de los números del 1 al  6, mostrar el número con su cuadrado");
		for (int i=1;i<7; i++) {
			System.out.print(Math.pow(i, 2));
			
		}
	}

}
